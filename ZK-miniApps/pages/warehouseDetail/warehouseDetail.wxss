/* pages/warehouseDetail/warehouseDetail.wxss */
.banner .swipers {
  height: 560rpx;
}

.slide-image {
  width: 100%;
  height: 100%;
}

.ware-detail .summary {
  padding: 0 30rpx;
  background: #fff;
}

.ware-detail .summary .titleContainer {
  display: flex;
  padding-top: 26rpx;
}

.ware-detail .summary .titleContainer .title {
  display: flex;
  flex: 1;
  height: 80rpx;
  font-size: 40rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  line-height: 48rpx;
}

.ware-detail .summary .titleContainer .title .tip {
  padding: 2rpx 8rpx;
  font-size: 20rpx;
  line-height: 28rpx;
  color: #fff;
  background: #fd714d;
  margin-right: 15rpx;
  position: relative;
  top: -2rpx;
}

.ware-detail .summary .titleContainer .share {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 116rpx;
  border-left: 2rpx solid #F1F1F1;
}

/* .ware-detail .summary .titleContainer .share image {
  width: 40rpx;
  height: 40rpx;
  margin-bottom: 16rpx;
} */

.ware-detail .summary .price {
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx 0 20rpx 0;
}

.ware-detail .summary .price .left {
  display: flex;
  align-items: center;
  font-size: 46rpx;
  color: #ff500b;
  margin: 10rpx;
}

.ware-detail .summary .price .left>view {
  display: flex;
  align-items: center;
  font-size: 24rpx;
  color: #57616d;
  margin-left: 5rpx;
  margin-top: 8rpx;
}

.ware-detail .summary .price .right {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 115rpx;
  height: 35rpx;
  border: 1rpx solid #ffdcce;
  color: #ff500b;
  font-size: 24rpx;
  border-radius: 4rpx;
}

.ware-detail .summary .price .right image {
  width: 18rpx;
  height: 22rpx;
  margin-right: 6rpx;
  margin-top: 3rpx;
}

.ware-detail .ware-area {
  height: 130rpx;
  align-items: center;
  display: flex;
  background: #fff;
  font-size: 30rpx;
  color: #2d323c;
  border-bottom: 1rpx solid #f1f1f1;
}

.ware-detail .ware-area .item {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.ware-detail .ware-area .border {
  width: 2rpx;
  height: 80rpx;
  background: #f1f1f1;
}

.ware-detail .ware-area .item view:last-child {
  color: #818ea1;
  font-size: 22rpx;
  margin-top: 3rpx;
}

.ware-detail .ware-tips {
  height: 100rpx;
  background: #fff;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}

.ware-detail .ware-tips .item {
  padding: 0 23rpx;
  height: 46rpx;
  border-radius: 100rpx;
  line-height: 46rpx;
  font-size: 24rpx;
  color: #ff500b;
  background: #fff6f3;
  margin-right: 22rpx;
  letter-spacing: 2rpx;
}

.ware-detail .some-info {
  background: #fff;
  font-size: 26rpx;
  padding-left: 30rpx;
  padding-right: 40rpx;
}

.ware-detail .some-info .item {
  display: flex;
  color: #2d323c;
  position: relative;
  padding-bottom: 30rpx;
}

.ware-detail .some-info .item .left {
  width: 185rpx;
  color: #818ea1;
}

.ware-detail .some-info .item .center {
  flex: 1;
  margin-right: 20rpx;
}

.ware-detail .some-info .item .right {
  position: absolute;
  right: 0;
  top: 0;
  display: flex;
  align-items: center;
  color: #fd714d;
}

.ware-detail .some-info .item .right image {
  width: 15rpx;
  height: 25rpx;
  margin-left: 20rpx;
}

.ware-detail .ware-tab {
  height: 80rpx;
  background: #fff;
  display: flex;
  align-items: center;
  font-size: 26rpx;
  color: #2d323c;
  padding: 0 15rpx;
  justify-content: space-around;
  line-height: 76rpx;
  border-bottom: 1rpx solid #f1f1f1;
  position: sticky;
  top: 0;
  z-index: 1;
}

.ware-detail .ware-tab .active {
  color: #fd714d;
  border-bottom: 4rpx solid #fd714d;
}

.ware-detail .detail-group {
  height: 90vh;
  overflow: hidden;
  position: sticky;
  top: 80rpx;
}

.ware-detail .ware-content {
  background: #fff;
}

.ware-detail .ware-content .title {
  margin-left: 30rpx;
  font-size: 30rpx;
  color: #2d323c;
  font-weight: 600;
  height: 110rpx;
  line-height: 110rpx;
}

.ware-detail .bottom {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  z-index: 1;
  position: fixed;
  bottom: 0;
  background: #fff;
}

.ware-detail .bottom .left {
  flex: 1;
  display: flex;
  align-items: center;
  padding: 0 15rpx;
  justify-content: space-around;
  font-size: 18rpx;
  color: #555;
}

.ware-detail .bottom .left .item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.ware-detail .bottom .left .item image {
  width: 33rpx;
  height: 35rpx;
  margin-bottom: 8rpx;
}

.ware-detail .bottom .center {
  width: 230rpx;
  height: 100rpx;
  color: #fff;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.ware-detail .bottom .place {
  background: #ffa861;
}

.ware-detail .bottom .appoint {
  background:#fd714d;
}

.ware-detail .bottom .appoint image {
  width: 35rpx;
  height: 35rpx;
}

.float {
  width: 121rpx;
  height: 121rpx;
  background: #fd714d;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  color: #fff;
  font-size: 30rpx;
  position: fixed;
  right: 34rpx;
  bottom: 161rpx;
  z-index: 2;
}

.info {
  font-size: 26rpx;
  color: #2D323C;
  padding: 0 30rpx;
  line-height: 36rpx;
}

.status {
  width: 100%;
  font-size: 24rpx;
  background-color: rgba(0,0,0,0.4);
  color: #FFFFFF;
  height: 64rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 1;
}
.choose{
  border-top: 1rpx solid #fafafa;
  border-bottom: 1rpx solid #fafafa;
  padding:5%;
  font-size: 30rpx;
}
.inchoo{
  display: flex;
  margin-bottom: 20rpx;
}
.le{
  flex: 10;
}
.ri{
  flex: 1;
  margin-left: 100rpx;
}
.jian {
  width: 15rpx;
  height: 20rpx;
}

.mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 5;
  background: rgba(0, 0, 0, 0.5);
}
 
 
 
/*价格信息*/
.goods-name .goname-title {
  width: 600rpx;
}
/*弹窗骨架*/
.lay-box {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100vw;
  background: #fff;
  z-index: 100;
}
.lay-box {
  transform: translate(0, 100%);
  transition: all 0.3s ease-in;
}
.lay-box.on {
  transform: translate(0, 0);
} 
.detail-laybox .box-title {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 28rpx;
  border-bottom: 1px solid #e5e5e5;
}
.box-content {
  max-height: 500rpx;
}
.detail-laybox .no-params {
  padding: 30rpx;
  text-align: center;
  color: #666;
}
.detail-laybox .box-btn,
.detail-cart .box-btn {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  background: #f13130;
  color: #fff;
  font-size: 30rpx;
} 
/*购物车弹窗*/
.detail-cart .box-cart-title {
  padding: 20rpx;
  display: flex;
  border-bottom: 1px solid #e5e5e5;
} 
.detail-cart .box-cart-title .box-img {
  width: 170rpx;
  height: 170rpx;
  border-radius: 10rpx;
  margin-top: 0rpx;
  margin-right: 20rpx;
}
.detail-cart .box-cart-title .price-box {
  flex: 1;
  position: relative;
} 
.detail-cart .box-cart-title .price-box .price {
  font-weight: bold;
  font-size: 32rpx;
  color: #f00;
}
.detail-cart .box-cart-title .price-box .sales {
  color: #888;
} 
.detail-cart .box-cart-title .price-box .lay-close {
  position: absolute;
  right: 0;
  top: 0;
  width: 28rpx;
  height: 28rpx;
}
.detail-cart .box-spec {
  padding: 20rpx;
  border-bottom: 1px solid #ddd;
} 
.detail-cart .box-spec .spec-items {
  display: flex;
  flex-wrap: wrap;
  padding-right: -40rpx;
  margin-top: 12rpx;
}
.detail-cart .box-spec .spec-items button {
  margin-left: 0;
}
.detail-cart .box-spec .spec-items button::after {
  border: 0;
}
.detail-cart .box-spec .spec-item {
  height: 50rpx;
  line-height: 46rpx;
  padding: 0 20rpx;
  font-size: 26rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  border: 2rpx solid #e5e5e5;
  border-radius: 6rpx;
  background-color: #fff;
  color: #000;
}
.detail-cart .box-spec .spec-item.on {
  color: #ff5000;
  border-color: #ff5000;
}
.detail-cart .box-spec .spec-items button.on {
  color: #ff5000;
  border-color: #ff5000;
}
.detail-cart .box-spec .spec-item.grey {
  background: #f1f1f1;
}
.updown-box {
  padding: 20rpx;
  border-top: 1px solid #e5e5e5;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
/*数量加减*/
.updown {
  border: 1px solid #e5e5e5;
  border-radius: 6rpx;
  overflow: hidden;
  height: 70rpx;
  display: flex;
  width: 200rpx;
  font-size: 40rpx;
  align-items: center;
}
.updown .up,
.updown .down {
  width: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100%;
}
.updown .up {
  border-left: 1rpx solid #e5e5e5;
}
.updown .down {
  border-right: 1rpx solid #e5e5e5;
}
.updown .nums {
  width: 80rpx;
  text-align: center;
  height: 100%;
  display: inline-block;
  font-size: 30rpx;
}
.cor-grey{
  font-size: 30rpx;
}
.sharebtn{
  height: 110rpx;
  margin-top: 20rpx;
  background-color: white;
  color: #999999;
  border-radius: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.sharebtn::after {
  border: 0; 
}
.shimg{
  width: 50rpx;
  height: 50rpx;
}
.oldprice{
  text-decoration:line-through;
  font-size: 35rpx;
  color: rgb(201, 199, 199);
  flex: 1;
  margin-top: 12rpx;
  margin-right: 5rpx;
}